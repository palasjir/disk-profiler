{
  "name": "disk-profiler",
  "version": "1.0.0",
  "main": "main.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "run-p 'dev:*'",
    "dev:electron": "electron .",
    "dev:renderer": "webpack --config webpack.config.renderer.js --watch --mode=development",
    "dev:main": "webpack --config webpack.config.main.js --watch --mode=development",
    "build": "yarn build:main && yarn build:renderer",
    "build:renderer": "webpack --config webpack.config.renderer.js --mode=production",
    "build:main": "webpack --config webpack.config.main.js --mode=production",
    "start": "yarn build && electron .",
    "test": "jest",
    "scan": "ts-node ./scripts/scan.ts"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "electron": "^6.0.8",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "typescript-plugin-styled-components": "^1.4.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  },
  "dependencies": {
    "@material-ui/core": "^4.4.2",
    "@material-ui/icons": "^4.4.1",
    "@types/node": "^12.7.5",
    "@types/styled-components": "^4.1.19",
    "chokidar": "^3.1.0",
    "clsx": "^1.0.4",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.3.2",
    "ts-node": "^8.4.1"
  }
}
